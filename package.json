{
  "name": "kayvee",
  "version": "0.1.0",
  "description": "NPM module for compiling TailwindCSS.",
  "main": "tailwind.config.js",
  "scripts": {
    "build": "npx tailwindcss -i ./tailwind.css -o ./kayvee/static/app.css",
    "watch": "npx tailwindcss -i ./tailwind.css -o ./kayvee/static/app.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t-eckert/kayvee.git"
  },
